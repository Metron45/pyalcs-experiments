   "source": [
    "from lcs.agents.xcs import XCS, Configuration\n",
    "from collections import namedtuple\n",
    "\n",
    "# requires knowledge of SimpleMazeRP properties\n",
    "# The XCS raises numerosity of \"good\" clasifiers,\n",
    "# while deleting not needed rules\n",
    "def xcs_simple_maze_knowledge(xcs, environment):\n",
    "    good_cls = [\n",
    "        [\"1001\", 1],\n",
    "        [\"1010\", 1],\n",
    "        [\"1100\", 2],\n",
    "        [\"0111\", 0],\n",
    "        [\"1101\", 2],\n",
    "        [\"1010\", 2],\n",
    "        [\"1011\", 0],\n",
    "        [\"0010\", 3],\n",
    "        [\"0110\", 3],\n",
    "    ]\n",
    "    good_cl_in_pop = 0\n",
    "    for cl in xcs.population:\n",
    "        for gcl in good_cls:\n",
    "            if cl.does_match(gcl[0]) and cl.action == gcl[1]:\n",
    "                good_cl_in_pop += cl.numerosity\n",
    "    return good_cl_in_pop / xcs.cfg.theta_mna\n",
    "    \n",
    "\n",
    "def xcs_simple_maze_metrics(xcs: XCS, environment):\n",
    "    return {\n",
    "        'numerosity': xcs.population.numerosity(),\n",
    "        'knowledge': xcs_simple_maze_knowledge(xcs, environment),\n",
    "        'population': len(xcs.population)\n",
    "    }\n",
    "\n",
    "\n",
    "cfg = Configuration(theta_mna=4,\n",
    "                    n=1000,\n",
    "                    epsilon=0.5,\n",
    "                    user_metrics_collector_fcn=xcs_simple_maze_metrics)\n",
    "\n",
    "agent = XCS(cfg)\n",
    "population, explore_metrics = agent.explore(maze, 10000, False)\n"
   ]
  },
